<h1></h1>

<h1 style ="text-align: center;">update your profile!</h1>

<%= form_for current_renter do |f|%>
<%= f.label :name%><br>
  <%= f.text_field :name%><br>
  <%= f.label :address%><br>
  <%= f.text_field :address%><br>
  <%= f.label :age%><br>
  <%= f.text_field :age%><br>
  <%= f.label :phone_number%><br>
  <%= f.text_field :phone_number%><br>
  <%= f.label :username%><br>
  <%= f.text_field :username%><br>
  <%= f.label :password%><br>
  <%= f.password_field :password%><br>
  <%= f.submit('create renter', :class => "style_submit") %>
<%end%>