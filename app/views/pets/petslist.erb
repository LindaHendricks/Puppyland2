<h1></h1>
<h2 style ="text-align: center;">Search for Pets by Name:</h2>
    <%= form_tag(petslist_path, method: :get) do %>
        <%= text_field_tag(:search, params[:search]) %> 
        <%= submit_tag ("Search")%> 
<% end %>  

